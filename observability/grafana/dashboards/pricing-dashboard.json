{
  "dashboard": {
    "id": null,
    "title": "FastAPI Enterprise - Pricing System Dashboard",
    "tags": ["pricing", "fastapi", "enterprise"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Pricing Calculations Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(pricing_calculations_total[5m])",
            "legendFormat": "Calculations/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(pricing_errors_total[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Active Calculations",
        "type": "stat",
        "targets": [
          {
            "expr": "active_pricing_calculations",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Limit Violations Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(pricing_limit_violations_total[5m])",
            "legendFormat": "Violations/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Calculation Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(pricing_calculation_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(pricing_calculation_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(pricing_calculation_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {
            "unit": "s",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Pricing by Material",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (material) (rate(pricing_calculations_total[5m]))",
            "legendFormat": "{{material}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Final Prices Distribution",
        "type": "histogram",
        "targets": [
          {
            "expr": "pricing_final_prices",
            "legendFormat": "Final Prices"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Margins Distribution",
        "type": "histogram",
        "targets": [
          {
            "expr": "pricing_margins",
            "legendFormat": "Margins"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Calculations by Customer Tier",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum by (customer_tier) (rate(pricing_calculations_total[5m]))",
            "legendFormat": "{{customer_tier}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Calculations by Process",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum by (process) (rate(pricing_calculations_total[5m]))",
            "legendFormat": "{{process}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24}
      },
      {
        "id": 11,
        "title": "Tier Selection Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (tier) (rate(pricing_final_prices_count[5m]))",
            "legendFormat": "{{tier}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24}
      }
    ]
  },
  "folderId": null,
  "overwrite": true
}
